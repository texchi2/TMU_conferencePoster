journal = {Journal of Clinical Investigation},
number = {2},
pages = {135--142},
pmid = {11160126},
publisher = {The American Society for Clinical Investigation},
title = {{Therapeutic potential of inhibition of the NF-$\kappa$B pathway in the treatment of inflammation and cancer}},
volume = {107},
year = {2001}
}
@article{Patel2010,
