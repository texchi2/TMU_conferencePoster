issn = {0022-538X},
journal = {Journal of Virology},
keywords = {Epstein-Barr virus,KRAB-ZFP,Kaposi's sarcoma-associated herpesvirus,STAT3,SZF1,TRIM28,ZNF557,lytic cycle,viral persistence},
mendeley-tags = {ZNF557},
number = {14},
