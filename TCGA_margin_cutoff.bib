author = {Hong, J H},
doi = {10.1016/j.jtho.2018.10.055},
issn = {1556-0864},
journal = {Journal of Thoracic Oncology},
keywords = {VEGFC,integrin subunit alpha 5,lung adenocarcinoma,validation},
