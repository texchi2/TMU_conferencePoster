issn = {11786930},
journal = {OncoTargets and Therapy},
keywords = {Acute myeloid leukemia,Bioinformatics analysis,DEGs,GSEA,RNA sequencing,TCGA,TP53 mutation},
mendeley-tags = {DEGs,GSEA},
