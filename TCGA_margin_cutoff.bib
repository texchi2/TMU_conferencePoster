title = {{EpCAM Signaling Promotes Tumor Progression and Protein Stability of PD-L1 through the EGFR Pathway}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/32978170 http://cancerres.aacrjournals.org/lookup/doi/10.1158/0008-5472.CAN-20-1264},
