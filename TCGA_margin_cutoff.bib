issn = {15384047},
journal = {Cancer Biology and Therapy},
keywords = {Cytokine,JNK activation,Overexpression,Pancreatic cancer,Proinflammation,Thymosin beta 4,Tumor tissue},
number = {3},
pages = {419--423},
pmid = {18094619},
title = {{Thymosin beta 4 is overexpressed in human pancreatic cancer cells and stimulates proinflammatory cytokine secretion and JNK activation}},
