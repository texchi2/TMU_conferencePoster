publisher = {Public Library of Science},
title = {{Characteristics and prognosis of primary treatment-na{\"{i}}ve oral cavity squamous cell carcinoma in Norway, a descriptive retrospective study}},
url = {https://doi.org/10.1371/journal.pone.0227738},
volume = {15},
year = {2020}
}
