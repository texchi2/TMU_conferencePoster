title = {{Detection of conserved segments in proteins: Iterative scanning of sequence databases with alignment blocks}},
url = {https://www.pnas.org/content/100/16/9440 http://www.ncbi.nlm.nih.gov/pubmed/7991589 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC45382},
