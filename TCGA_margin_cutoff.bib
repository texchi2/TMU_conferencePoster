keywords = {Biomarkers, Tumor,DNA Methylation,DNA, Neoplasm,Female,Genome-Wide Association Study,Humans,Male,Neoplasm Proteins,Oropharyngeal Neoplasms,Papillomaviridae,Papillomavirus Infections,genetics,metabolism,pathology,virology},
