file = {:Users/texchi/Downloads/cancers-13-02387.pdf:pdf},
issn = {2072-6694 (Print)},
journal = {Cancers},
keywords = {HNSCC,HPA},
language = {eng},
mendeley-tags = {HNSCC,HPA},
month = {may},
