pmid = {30205384},
publisher = {Cell Physiol Biochem},
title = {{MiR-182-5p Promotes Growth in Oral Squamous Cell Carcinoma by Inhibiting CAMK2N1}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/30205384},
volume = {49},
year = {2018}
}
