issn = {19160216},
journal = {Journal of Otolaryngology - Head and Neck Surgery},
keywords = {EGFR,HNSCC,HPV,Oropharyngeal cancer,Smoking,Survival,biomarker,tobacco},
mendeley-tags = {HNSCC,biomarker,tobacco},
month = {dec},
