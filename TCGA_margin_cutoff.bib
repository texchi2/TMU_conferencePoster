journal = {Applied Sciences (Switzerland)},
keywords = {Cancer classification,Gene selection,L1/2 regularization,Regularized logistic regression},
number = {9},
pages = {1--12},
title = {{Gene selection in cancer classification using sparse logistic regression with L1/2 regularization}},
volume = {8},
year = {2018}
}
