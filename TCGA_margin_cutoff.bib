keywords = {igemdock},
mendeley-tags = {igemdock},
number = {SUPPL. 1},
pages = {S33},
pmid = {21342564},
title = {{Igemdock: A graphical environment of enhancing gemdock using pharmacological interactions and post-screening analysis}},
url = {http://bmcbioinformatics.biomedcentral.com/articles/10.1186/1471-2105-12-S1-S33},
volume = {12},
