title = {{GDCRNATools: An R/Bioconductor package for integrative analysis of lncRNA, miRNA and mRNA data in GDC}},
url = {http://dx.doi.org/10.1101/229799 https://academic.oup.com/bioinformatics/advance-article/doi/10.1093/bioinformatics/bty124/4917355},
volume = {34},
