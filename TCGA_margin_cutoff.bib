issn = {2234943X},
keywords = {cancer,prognosis,survival,tool,web server},
month = {feb},
pages = {68},
publisher = {Frontiers},
title = {{Comprehensive Review of Web Servers and Bioinformatics Tools for Cancer Prognosis Analysis}},
url = {https://www.frontiersin.org/article/10.3389/fonc.2020.00068/full},
volume = {10},
year = {2020}
