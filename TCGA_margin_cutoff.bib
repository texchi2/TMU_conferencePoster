file = {:Users/texchi/Library/Application Support/Mendeley Desktop/Downloaded/�kervall et al. - 1997 - Amplification of cyclin D1 in squamous cell carcinoma of the head and neck and the prognostic value of chromo.pdf:pdf},
issn = {0008-543X},
journal = {Cancer},
number = {2},
pages = {380--389},
title = {{Amplification of cyclin D1 in squamous cell carcinoma of the head and neck and the prognostic value of chromosomal abnormalities and cyclin D1 overexpression}},
url = {https://acsjournals.onlinelibrary.wiley.com/doi/full/10.1002/{\%}28SICI{\%}291097-0142{\%}2819970115{\%}2979{\%}3A2{\%}3C380{\%}3A{\%}3AAID-CNCR22{\%}3E3.0.CO{\%}3B2-W},
volume = {79},
