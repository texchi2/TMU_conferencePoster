author = {Vermorken, Jan B. and Mesia, Ricard and Rivera, Fernando and Remenar, Eva and Kawecki, Andrzej and Rottey, Sylvie and Erfan, Jozsef and Zabolotnyy, Dmytro and Kienzer, Heinz Roland and Cupissol, Didier and Peyrade, Frederic and Benasso, Marco and Vynnychenko, Ihor and {De Raucourt}, Dominique and Bokemeyer, Carsten and Schueler, Armin and Amellal, Nadia and Hitt, Ricardo},
